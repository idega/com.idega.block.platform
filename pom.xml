    <!-- XML APIs start
    Xerces and Xalan both depend on the serializer jar that is shipped with Xalan (and has the same version).
    Both also use the xml-api jar (Xerces implements it). So make sure to use only compatible versions of these jars -->
    <dependency>
      <groupId>xerces</groupId>
      <artifactId>xercesImpl</artifactId>
      <version>${xerces-xercesImpl-version}</version>
      <type>jar</type>
    </dependency>
    
