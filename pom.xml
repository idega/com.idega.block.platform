      <artifactId>xalan-serializer</artifactId>
      <version>${xalan-version}</version>
      <type>jar</type>
    </dependency>
    
    <dependency>
      <groupId>xml-apis</groupId>
      <artifactId>xml-apis</artifactId>
      <version>${xml-apis-xml-apis-version}</version>
      <type>jar</type>
    </dependency>
